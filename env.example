# RAG Insights Engine - Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI Configuration (Required)
OPENAI_API_KEY=your_openai_api_key

# Optional Configuration (with defaults)
# OPENAI_MODEL=gpt-4o-mini
# EMBEDDING_MODEL=gte-small

# Search Configuration
# SIMILARITY_THRESHOLD=0.6  # Minimum similarity score for search results (0.0-1.0)
# MAX_CHUNKS=50  # Maximum number of chunks to retrieve per search
# RRF_K_VALUE=10  # Reciprocal Rank Fusion parameter for combining search results (higher = more balanced)
